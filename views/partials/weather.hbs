<div class="weatherWidget">
    {{#each partials.weather.locations as |locations|}}
        <div class="location">
            <h3>{{name}}</h3>
            <a href="{{forecastUrl}}">
                <img src="{{iconUrl}}" alt="{{weather}}">
                {{weather}},{{temp}}
            </a>
        </div>
    {{/each}}
</div>